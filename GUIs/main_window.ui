<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object id="main_window" class="ttk.Frame">
    <property name="height">600</property>
    <property name="width">800</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">False</property>
      <property name="row">0</property>
      <rows>
        <row id="0">
          <property name="minsize">0</property>
          <property name="pad">0</property>
          <property name="weight">2</property>
        </row>
      </rows>
      <columns>
        <column id="0">
          <property name="minsize">0</property>
          <property name="pad">0</property>
        </column>
      </columns>
    </layout>
    <child>
      <object id="label_title" class="ttk.Label">
        <property name="font">{Times New Roman} 24 {}</property>
        <property name="text" translatable="yes">Tanks explorer v0.3</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object id="Frame_1" class="ttk.Frame">
        <property name="height">400</property>
        <property name="width">700</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">False</property>
          <property name="row">2</property>
        </layout>
        <child>
          <object id="Frame_2" class="ttk.Frame">
            <property name="height">400</property>
            <property name="padding">30</property>
            <property name="width">800</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
              <rows>
                <row id="0">
                  <property name="pad">5</property>
                </row>
                <row id="1">
                  <property name="pad">5</property>
                </row>
                <row id="2">
                  <property name="pad">5</property>
                </row>
                <row id="3">
                  <property name="pad">5</property>
                </row>
                <row id="4">
                  <property name="pad">5</property>
                </row>
              </rows>
              <columns>
                <column id="0">
                  <property name="minsize">0</property>
                  <property name="pad">20</property>
                </column>
              </columns>
            </layout>
            <child>
              <object id="button_list" class="tk.Button">
                <property name="command">list_func</property>
                <property name="height">3</property>
                <property name="text" translatable="yes">List of tanks</property>
                <property name="width">35</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object id="button_compare" class="tk.Button">
                <property name="command">compare_func</property>
                <property name="font">TkDefaultFont</property>
                <property name="height">3</property>
                <property name="justify">left</property>
                <property name="text" translatable="yes">Compare tanks</property>
                <property name="width">35</property>
                <layout>
                  <property name="column">1</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object id="button_add" class="tk.Button">
                <property name="command">add_func</property>
                <property name="height">3</property>
                <property name="text" translatable="yes">Add tank</property>
                <property name="width">35</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object id="button_edit" class="tk.Button">
                <property name="command">edit_func</property>
                <property name="height">3</property>
                <property name="text" translatable="yes">Edit</property>
                <property name="width">35</property>
                <layout>
                  <property name="column">1</property>
                  <property name="propagate">True</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object id="button_save" class="tk.Button">
                <property name="anchor">center</property>
                <property name="command">save_func</property>
                <property name="default">normal</property>
                <property name="height">3</property>
                <property name="text" translatable="yes">Save</property>
                <property name="width">35</property>
                <layout>
                  <property name="column">0</property>
                  <property name="columnspan">2</property>
                  <property name="propagate">True</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object id="exit_button" class="tk.Button">
                <property name="command">exit_func</property>
                <property name="default">normal</property>
                <property name="height">3</property>
                <property name="text" translatable="yes">Exit</property>
                <property name="width">35</property>
                <layout>
                  <property name="column">0</property>
                  <property name="columnspan">2</property>
                  <property name="propagate">True</property>
                  <property name="row">3</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object id="Label_1" class="ttk.Label">
        <property name="width">7</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object id="Label_2" class="ttk.Label">
        <property name="text" translatable="yes">
</property>
        <layout>
          <property name="column">2</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
